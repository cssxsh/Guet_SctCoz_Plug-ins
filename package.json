{
    "name": "guet_sctcoz_plug-ins",
    "version": "1.0.0",
    "description": "This is CssXsh's first public project.\r ## 2019/03/21: \r     Learn Linux version number representation.",
    "main": "index.js",
    "devDependencies": {
        "eslint": "^6.5.1",
        "eslint-config-google": "^0.14.0",
        "eslint-config-prettier": "^6.4.0",
        "eslint-plugin-prettier": "^3.1.1",
        "husky": "^3.0.9",
        "lint-staged": "^9.4.2",
        "prettier": "^1.18.2",
        "prettier-check": "2.0.0",
        "pretty-quick": "2.0.0"
    },
    "scripts": {
        "format": "eslint \"Bate/*.js\" --fix  && prettier \"Bate/*.{js,json,md}\" --write",
        "format:staged": "eslint \"Bate/*.js\" --fix && pretty-quick --staged --verbose --pattern \"Bate/*.{js,json,md}\"",
        "format:check": "eslint \"Bate/*.js\" && prettier-check \"Bate/*.{js,json,md}\""
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run format"
        }
    },
    "prettier": {
        "printWidth": 256,
        "tabWidth": 4,
        "singleQuote": true,
        "trailingComma": "es5",
        "arrowParens": "always"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cssxsh/Guet_SctCoz_Plug-ins.git"
    },
    "author": "cssxsh",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/cssxsh/Guet_SctCoz_Plug-ins/issues"
    },
    "homepage": "https://github.com/cssxsh/Guet_SctCoz_Plug-ins#readme"
}
