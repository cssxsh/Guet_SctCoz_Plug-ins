            "iconColour": "red",
            "type": "line",
            "foreground": "red"
        },
        "FIXME": {
            "icon": "beaker",
            "iconColour": "yellow",
            "type": "line",
            "foreground": "yellow"
        },
        "XXX": {
            "icon": "check",
            "iconColour": "blue",
            "type": "line",
            "foreground": "blue"
        }
    }
