        "TODO": {
            "icon": "alert",
            "iconColour": "red",
            "type": "line",
            "foreground": "red"
        },
        "FIXME": {
            "icon": "beaker",
            "iconColour": "yellow",
            "type": "line",
            "foreground": "yellow"
        },
