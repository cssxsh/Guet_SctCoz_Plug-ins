{
    "todo-tree.general.tags": ["TODO", "FIXME", "XXX"],
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            "icon": "alert",
            "iconColour": "red",
            "type": "line",
            "foreground": "red"
        },
        "FIXME": {
            "icon": "beaker",
            "iconColour": "yellow",
            "type": "line",
            "foreground": "yellow"
        },
        "XXX": {
            "icon": "check",
            "iconColour": "blue",
            "type": "line",
            "foreground": "blue"
        }
    },
    "files.autoSave": "afterDelay",
    "diffEditor.ignoreTrimWhitespace": true,
    "window.zoomLevel": 0,
    "prettier.arrowParens": "always",
    "prettier.proseWrap": "never",
    "prettier.printWidth": 256
}